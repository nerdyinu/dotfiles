  # Custom (Random emoji) 
  emojis=("âš¡ï¸" "ðŸ”¥" "ðŸ‡°" "ðŸ‘‘" "ðŸ˜Ž" "ðŸ¸" "ðŸµ" "ðŸ¦„" "ðŸŒˆ" "ðŸ»" "ðŸš€" "ðŸ’¡" "ðŸŽ‰" "ðŸ”‘" "ðŸš¦" "ðŸŒ™")
  RAND_EMOJI_N=$(( $RANDOM % ${#emojis[@]} + 1)) 
